{
    "name": "invertus/venipak-api-client",
    "description": "Invertus Venipak api client",
    "type": "api-client",
    "license": "MIT",
    "version": "1.0.4",
    "autoload": {
        "psr-4": {
            "Invertus\\VenipakApiClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Invertus\\VenipakApiClient\\Tests\\": "src/Test"
        }
    },
    "require": {
        "php": "^8.1",
        "sammyjo20/saloon": "^2.5",
        "ext-simplexml": "*"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.6"
    }
}
